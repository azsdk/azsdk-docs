{
  "build": {
    "content": [
      {
        "files": [
          "**.md",
          "**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
		      "../Release/_AzSDKWebSite/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "Images/**"
        ],
        "exclude": [
          "obj/**",
		      "../Release/_AzSDKWebSite/**"
        ]
      }
    ],
    "dest": "../Release/_AzSDKWebSite",
    "overwrite": [],
    "globalMetadata": {
      "_appTitle": "AzSDK website",
      "brand": "azure",
      "searchScope": [ "Azure" ],
      "_gitContribute": {
        "repo": "https://github.com/azsdk/azsdk-docs",
        "branch": "master",
        "apiSpecFolder": "azsdk-docs"
      },
      "_gitUrlPattern": "github",
      "_appLogoPath": "Images/Icons/AzureSecurityDevOps_Logo.png",
      "_appFooter": "Copyright (c) - Microsoft 2017. All rights reserved.",
      "_appFaviconPath": "Images/AzSDK_Logo.png"
    },
    "globalMetadataFiles":[],
    "fileMetadataFiles": [],
    "template": [
       "../Artifacts/_exported_templates/AzSDKTemplate"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}